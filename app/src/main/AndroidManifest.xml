<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.freak.mvvmhttpmanager">

    <application
        android:name=".app.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".mvvm.activity.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mvvm.fregment.MVVMFragmentActivity" />
        <activity android:name=".down.DownActivity" />
        <activity android:name=".down.SystemDownloadActivity" />
        <activity android:name=".down.DownTaskListActivity" />

        <receiver android:name=".down.DownLoadCompleteReceiver" />
        <activity android:name=".mvvm.adapter.view.MVVMRecycleViewActivity" />
        <activity android:name=".mvvm.moredatabinding.view.MoreDatabindingActivity"/>
    </application>

</manifest>